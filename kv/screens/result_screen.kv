<ResultScreen>:
    BoxLayout:
        orientation: 'vertical'

        UpperBar:
            size_hint: (1, 0.1)
            canvas.before:
                Color:
                    rgb:(148/255, 193/255, 1, 1)
                Rectangle:
                    pos: self.pos
                    size: self.size

        BoxLayout:
            orientation:'vertical'
            size_hint:(1, 0.9)
            canvas:
                Color:
                    rgba:(1,1,1,1)
                Rectangle:
                    pos: self.pos
                    size: self.size






            Label:
                size_hint:(1, .1)